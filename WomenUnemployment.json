{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Women's Unemployment",
    "subtitle": "Trends in unemployment rates across different countries",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "middle",
    "color": "darkblue"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/Leenayesh/Leenayesh.github.io/main/Unemployment_rates_and_women1.csv",
    "format": {"type": "csv"}
  },
  "transform": [
    {
      "filter": {
        "field": "Country",
        "oneOf": [
          "Australia",
          "Austria",
          "Belgium",
          "Canada",
          "France",
          "Sweden"
        ]
      }
    }
  ],
  "height": 300,
  "width": 300,
  "mark": {"type": "line", "point": false},
  "encoding": {
    "x": {
      "field": "Year",
      "type": "temporal",
      "title": null,
      "axis": {"grid": false}
    },
    "y": {
      "field": "Value",
      "type": "quantitative",
      "title": "unemployment rate",
      "axis": {"grid": false}
    },
    "facet": {
      "field": "BIRTH",
      "type": "nominal",
      "title": "foreign born (FB) vs native born (NB)",
      "axis": {"grid": false}
    },
    "color": {
      "field": "Country",
      "type": "nominal",
      "scale": {"scheme": "inferno"},
      "title": null,
      "legend": {"orient": "top", "fillColor": "#FcFdFd"}
    }
  },
  "config": {}
}
